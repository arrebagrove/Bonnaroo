<script>
  jQuery(document).ready(function($){
    
  (function(){
  var timeout;
  var showFacebookFriends = $('#ds-content').data('ds-hovercards-version') == 'facebook';

$.fn.lineup_bubble = function() {
  var $this = $(this);
  // it will be called from playe callback on schedule page as well
  // so we want to be sure that bubble is not added to ds-event-boxed
  if ($this.hasClass('band') || $this.hasClass('ds-event-lower')) {
    var has_bubble = $this.data('lineup-bubble-has-bubble');
    if (!has_bubble) {
      $this.append($('<span class="ds-text" />').text($this.text()));
      $this.data('lineup-bubble-has-bubble', true);
    }
  }
  return $this;
}

// this function is used for hovercards on festivals
// referenced from tooltip.js.erb
var goAwayFacebook = false;
var add_remove_click = function(event) {
  var $dsAddRem = $(this);
  var isBand = event.data.isBand, triggerID;
    for_facebook_app = $('body').hasClass('ds-facebook-app');
  
  if (!event.data.triggerID) {
    triggerID = $dsAddRem.attr('band_id');
  } else {
    triggerID = event.data.triggerID
  }

  var toggleURL = isBand ? ['/band/', triggerID, '/fans/toggle.js'].join('') : ['/vote/', triggerID, '/toggle.js'].join('');
  if (for_facebook_app) {
    var tab_id = $('body').attr("ds-tab-id");
    toggleURL = "/contest/pagetab/" + tab_id + "/toggle/" + triggerID;
  }

  if ($dsAddRem.hasClass('ds-disabled')) return false;

  $dsAddRem.addClass('ds-disabled');

  var params = {};

  var showFacebookFriends = $('#ds-content').data('ds-hovercards-version') == 'facebook';
  if (showFacebookFriends) {
    params.v6 = 1;
  } else {
    params.v3 = 1;
  }
  if ( $('body').hasClass('ds-popup-v8') ) {
    var data = isBand ? (params.v8 = 1) : (params.v8lol = 1);
  }
  
    }

var $metabar_add = $('#ds-metabar .ds-add-remove');
// metabar add/remove works only with band voting now, hence the isBand: true
$metabar_add.live('click', {isBand : true}, add_remove_click)


$('.ds-lineup > ul > li > a, .ds-schedule .ds-event-box, ul.ds-lineup > li > em  > span > a')
  .add('.ds-event-lower .ds-event-lower').hover(
  function() {
    var $this = $(this),
        triggerID = this.id,
        tooltipState = $this.data('ds-tooltip-state'),
        isBand = $this.hasClass('band');
        if (!isBand) {
          isBand = $this.hasClass('band-image');
        }
        lower_tooltip = $this.hasClass('band-image') ? ' ds-tooltip-lower' : '';
        for_facebook_app = $('body').hasClass('ds-facebook-app');

            $this.lineup_bubble();

    if ($this.hasClass('band-image')) {
      $this.addClass('ds-hover-image');
    }

    $this
      .addClass('ds-hover')
            .parent()
        .addClass('ds-active');

    // add new tooltip if only there is no tooltip already
    if (typeof tooltipState === 'undefined') {
      var $tooltip = $('<span class="ds-tooltip' + lower_tooltip + '">\
                          <span class="ds-arrow"></span>\
                          <span class="ds-arrow-bg"></span>\
                          <span class="ds-content"><span class="ds-tooltip-band-actions">Loading...</span></span>\
                        </span>');
      if (showFacebookFriends) {
        $tooltip.addClass('ds-tooltip-fb-friends');
      }
      $this
                .data('ds-tooltip-state', 'added')
        .prepend($tooltip);
      if ($("body").hasClass("ds-popup-v8")) {
        adjustToolTipPosition($this, $tooltip);
      }
    }

    if (timeout) clearTimeout(timeout);

    if ($this.hasClass('no-popup')) {
      $(this).find('.ds-tooltip, .ds-text').remove();
    } else if (tooltipState != 'loaded' && tooltipState != 'requesting') {
            timeout = setTimeout(function(){
        var has_song = $this.hasClass('ds-player-show-state');
        var loadURL = [(isBand ? "/band/" : "/event/"), triggerID, "/fpup"].join('');
        if (for_facebook_app) { 
          var tab_id = $('body').attr("ds-tab-id");
          loadURL = "/contest/pagetab/" + tab_id + "/hover/" + triggerID;
        }
        var params = {};
        if (has_song) {
          params.has_song = true;
        }
        if (showFacebookFriends) {
          params.version = 'fb';
        }
                params = $.param(params);
        params && ( loadURL += '?' + params );

        $this
          .data('ds-tooltip-state', 'requesting')
          .find('> .ds-tooltip > .ds-content')
          .load(loadURL, null, function() {
            $this.data('ds-tooltip-state', 'loaded');
            // update ds-song status
            if (has_song) {
              // grab song node
              var $ds_song = $this.find('.ds-song');
              // read player status, gs-player change callbacks put it here
              var player_state = $this.data('ds-player-state');

              if (player_state == 'playing') {
                $ds_song
                  .addClass('ds-currently-playing')
                  .removeClass('ds-currently-paused');
              } else if (player_state == 'paused') {
                $ds_song
                  .addClass('ds-currently-paused')
                  .removeClass('ds-currently-playing');
              }
            }

            $this.find('.ds-add-remove').live('click', {isBand : isBand, triggerID: triggerID}, add_remove_click);
          });
      }, 400);
    }

        $this
      .find('.ds-tooltip')
        .bind('click', function(e){
          e.preventDefault();
          if ($(e.target).is('a')) {
            location.href = e.target.href;
          }
        });

    
  },

  function() {
    // delete all previously created stuff
    var $this = $(this);

    $this
      .removeClass('ds-hover')
      .parent()
        .removeClass('ds-active')
    if ($this.hasClass('band-image')) {
      $this.removeClass('ds-hover-image');
    }
    
    if (timeout) clearTimeout(timeout);
  }
);
})();
  


(function(){
  var page_type = 'lineup',
    eventType = 'mousedown',
    page_event_id = 0;
  
    
    $('li.ds-share-fb')
    .find('a')
      .bind(eventType, function(){
        if(typeof t_string === 'undefined'){t_string = 0;} _gaq.push(['t2._trackPageview', '/vpv/epv/f-451/' + page_type + '/page/share-facebook/local/0/' + page_event_id + '/none/0/0/' + t_string + '/']);      });
  $('li.ds-share-tw')
    .find('a')
      .bind(eventType, function(){
        if(typeof t_string === 'undefined'){t_string = 0;} _gaq.push(['t2._trackPageview', '/vpv/epv/f-451/' + page_type + '/page/share-twitter/local/0/' + page_event_id + '/none/0/0/' + t_string + '/']);      });
  $('li.ds-share-em')
    .find('a')
      .bind(eventType, function(){
        if(typeof t_string === 'undefined'){t_string = 0;} _gaq.push(['t2._trackPageview', '/vpv/epv/f-451/' + page_type + '/page/share-email/local/0/' + page_event_id + '/none/0/0/' + t_string + '/']);      });
  $('li.ds-share-addthis')
    .find('a')
      .bind(eventType, function(){
        if(typeof t_string === 'undefined'){t_string = 0;} _gaq.push(['t2._trackPageview', '/vpv/epv/f-451/' + page_type + '/page/share-addthis/local/0/' + page_event_id + '/none/0/0/' + t_string + '/']);      });

    $('a.ds-band-add')
    .bind(eventType, function(){
      if(typeof t_string === 'undefined'){t_string = 0;} _gaq.push(['t2._trackPageview', '/vpv/epv/f-451/' + page_type + '/page/add-band/local/0/' + page_event_id + '/none/0/0/' + t_string + '/']);    });
  $('a.ds-add-remove')
    .live(eventType, function(){
      var page_event_id = $(this).attr('band_id');
      if(typeof t_string === 'undefined'){t_string = 0;} _gaq.push(['t2._trackPageview', '/vpv/epv/f-451/' + page_type + '/page/add-band-hovercard/local/0/' + page_event_id + '/none/0/0/' + t_string + '/']);    });
  $('a.ds-song')
    .live(eventType, function(){
      var page_event_id = $(this).attr('band_id');
      if(typeof t_string === 'undefined'){t_string = 0;} _gaq.push(['t2._trackPageview', '/vpv/epv/f-451/' + page_type + '/page/play-song/local/0/' + page_event_id + '/none/0/0/' + t_string + '/']);    });
    
    $('input.ds-comments-submit')
    .live(eventType, function(){
      var page_event_id = $(this).attr('band_id');
      if(typeof t_string === 'undefined'){t_string = 0;} _gaq.push(['t2._trackPageview', '/vpv/epv/f-451/' + page_type + '/page/comment/local/0/' + page_event_id + '/none/0/0/' + t_string + '/']);    });

})();


    // current hostname 
    var hostname_regex = new RegExp(location.hostname.split('.').slice(1).join('.'), 'i');
    
    $("a").each(function(){
      /* 
        we have to use "hostname" and not "href", because 
        there might be our hostname in get attribute/value pairs 
        like "http://twitter.com?status=go+to+HOSTNAME.COM"
      */
      if ( (!hostname_regex.test(this.hostname)) && !(this.href == "javascript:void(0);") ) {
        $(this)
          .addClass("external")
          .attr("target","_blank");
      }
    });

  });
</script>
