      <script>
        window.fbAsyncInit = function() {
                FB.init({
        appId   : '192031150893987',
        status  : false,
        cookie  : true,
        xfbml   : true,
        oauth   : true,
        version : 'v2.0'
      });

          
              if(window.location.pathname == "/login" || window.location.pathname == "/join"){
      FB.Event.subscribe('auth.statusChange', function(response) {
        if (response.status === 'connected') {
          if (response.authResponse) {
            sr = response.authResponse.signedRequest;
            window.location = '/facebook/process-login?fb_sr='+sr;
          }
        }
      });
    }
    

          
          $(document).ready( function() {
            $('.ds-facebook-logout').click(function() {
              FB.logout(function(response) {
                window.location = '/logout';
              });
            });
          });
        };
        (function(d){
          var js, id = 'facebook-jssdk'; if (d.getElementById(id)) {return;}
          js = d.createElement('script'); js.id = id; js.async = true;
          js.src = "//connect.facebook.net/en_US/sdk.js";
