<script>
// using window onload, so we remove bullets only after the font has loaded
// on the long term, something like google webfonts loader should be used
$(window).load(function() {
  $(".ds-lineup > ul > li").each(function() {
    var $this = $(this), $prev = $this.prev();
    if($this.position().left > $this.offsetParent().width() / 2){
      $this.addClass("ds-reverse-hover");
    }
    if ($prev.length < 1 || ($this.offset().top > $prev.offset().top)) {
      // if left offset is smaller than previous, we are at first element on the row
      $this.addClass('ds-first');
    }
  });
  if(typeof FB != "undefined"){
    FB.Canvas.setSize();
  }
});
</script>

</div>
