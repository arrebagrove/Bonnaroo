function getMonthName(d){
  var month=new Array();
  month[0]="January";
  month[1]="February";
  month[2]="March";
  month[3]="April";
  month[4]="May";
  month[5]="June";
  month[6]="July";
  month[7]="August";
  month[8]="September";
  month[9]="October";
  month[10]="November";
  month[11]="December";
  return month[d.getMonth()];
}

function getDayName(d){
  var day=new Array();
  day[0]="Sunday";
  day[1]="Monday";
  day[2]="Tuesday";
  day[3]="Wednesday";
  day[4]="Thursday";
  day[5]="Friday";
  day[6]="Saturday";
  return day[d.getDay()];
}

if(window.location.pathname.match(/\/events+\/(\d{4})+\/(\d{2})+\/(\d{2})/gi)){
  var newStr = window.location.pathname.replace(/\/events+\/(\d{4})+\/(\d{2})+\/(\d{2})/gi, "$1/$2/$3");
  var newStrSplit = newStr.split("/")
  if(newStrSplit.length > 2){
    if(newStr.slice(-1) == "/"){
      newStr = newStr.slice(0, - 1); 
    }
    var date = new Date(newStr);
    $('.ds-schedule').prepend("<h2 style='top:0px;' class='ds-lineup-header ds-lineup-header-320'>" + getDayName(date) + ", " + getMonthName(date) + " " + newStrSplit[2] + "</h2>");
  }
} else if(window.location.pathname.match(/schedule+\/(\d{4})+\/(\d{2})+\/(\d{2})/gi)){
  var newStr = window.location.pathname.replace(/\/user\/\d+\/schedule+\/(\d{4})+\/(\d{2})+\/(\d{2})/gi, "$1/$2/$3");
  var newStrSplit = newStr.split("/")
  if(newStrSplit.length > 2){
    var date = new Date(newStr);
    $('.ds-schedule').prepend("<h2 style='top:0px;' class='ds-lineup-header ds-lineup-header-320'>" + getDayName(date) + ", " + getMonthName(date) + " " + newStrSplit[2] + "</h2>");
  }
}


